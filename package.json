    "@angular/platform-server": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@angular/ssr": "^19.0.0",
    "express": "^4.18.2",
    "gsap": "^3.12.7",
    "lottie-web": "^5.12.2",
    "ngx-lottie": "^13.0.1",
    "rxjs": "~7.8.0",
